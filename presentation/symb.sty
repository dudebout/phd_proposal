\ProvidesPackage{symb}

\RequirePackage{ddbsymb}

\renewcommand{\range}{\rangeus}

\newcommand{\shat}{\hat{s}}
\newcommand{\hhat}{\hat{h}}

\newcommand{\agentdynamic}{\(x\nxt \drawn f \of {x, a, s}\)}

\newcommand{\agentsystemaddingdepthk}{
  \(
  \begin{aligned}
    x\nxt &\drawn f \of {x, a, s} \\
    a &\drawn \sigma \of {\alt<1-2>{h}{\vphantom{h}\textcolor<3>{alerted diagram.fg}{x, z}}}
  \end{aligned}
  \)
}

\newcommand{\agentsystemaddingmodel}{
  \(
  \begin{aligned}
    x\nxt &\drawn f \of {x, a, \alt<6-7>{\textcolor<6>{alerted diagram.fg}{\shat}}{s}} \\
    \onslide<2->{a &\drawn \sigma \of{h}}
  \end{aligned}
  \)
}

\newcommand{\agentisystem}{
  \(
  \begin{aligned}
    x\Ii\nxt &\drawn f\Ii \of {x\Ii, a\Ii, s\Ii} \\
    a\Ii &\drawn \sigma\Ii \of {x\Ii, z\Ii}
  \end{aligned}
  \)
}

\newcommand{\naturesystem}{
  \(
  \begin{aligned}
    w\nxt &\drawn n \of{w, x, a}\\
    s &\drawn \nu \of{w}
  \end{aligned}
  \)
}

\newcommand{\depthkmodelsystem}{
  \(
  \begin{aligned}
    z\nxt &\drawn m_{k} \of{z}\\
    \shat &\drawn \mu \of{z}
  \end{aligned}
  \)
}

\newcommand{\depthkmodelisystem}{
  \(
  \begin{aligned}
    z\Ii\nxt &\drawn m_{i,k} \of{z\Ii}\\
    \shat\Ii &\drawn \mu\Ii \of{z\Ii}
  \end{aligned}
  \)
}

\newcommand{\multiagentstateactionsignal}{
  \(
  \begin{aligned}
    x &= \tuple{\sseqcom{x}{\ag}{1}{2}{N}} \\
    a &= \tuple{\sseqcom{a}{\ag}{1}{2}{N}} \\
    s &= \tuple{\sseqcom{s}{\ag}{1}{2}{N}}
  \end{aligned}
  \)
}
